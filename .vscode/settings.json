{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.options": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "liveServer.settings.multiRootWorkspaceName": "sprouts",
    "liveServer.settings.port": 5501,
}